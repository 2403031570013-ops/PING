{
    "version": 2,
    "name": "lost-found-backend",
    "builds": [
        {
            "src": "src/app.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(api|uploads)(.*)",
            "dest": "src/app.js"
        },
        {
            "src": "/(.*)",
            "dest": "src/app.js"
        }
    ],
    "env": {
        "NODE_ENV": "production"
    }
}